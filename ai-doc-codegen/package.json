{
  "name": "ai-code-gen",
  "displayName": "AI Code Generator",
  "description": "A VS Code extension to generate AI-powered code snippets.",
  "version": "1.0.1",
  "publisher": "your-name",
  "engines": {
    "vscode": "^1.75.0",
    "node": ">=16.0.0"
  },
  "repository": {
  "type": "git",
  "url": "https://github.com/Kishan1610/AI-Code-Gen.git"
  },
  "categories": ["Other"],
  "contributes": {
    "commands": [
      {
        "command": "extension.openWebView",
        "title": "Open AI Code Generator"
      }
    ]
  },
  "activationEvents": ["onCommand:extension.openWebView"],
  "main": "extension.js",
  "scripts": {
    "prepublish": "vsce package",
    "package": "vsce package",
    "install": "npm install",
    "start": "code --extensionDevelopmentPath=.",
    "test": "echo 'No test specified'"
  },
  "devDependencies": {
  "@vscode/vsce": "^2.15.0",
  "vscode": "^1.1.36"
}

}
